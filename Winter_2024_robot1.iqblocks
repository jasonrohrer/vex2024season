,{"port":[2],"name":"Bumper2","customName":false,"deviceType":"Bumper","deviceClass":"bumper","setting":{},"triportSourcePort":22},{"port":[3],"name":"Distance3_new","customName":true,"deviceType":"Distance","deviceClass":"distance","setting":{}}],"slot":0,"platform":"IQ","sdkVersion":"20230818.11.00.00","appVersion":"3.0.1","minVersion":"3.0.0","fileFormat":"1.2.0","icon":"","targetBrainGen":"First","v5SoundsEnabled":false,"downloadLanguage":"python","cppStatus":"true","cpp":"// Make sure all required headers are included.\n#include <stdio.h>\n#include <stdlib.h>\n#include <stdbool.h>\n#include <math.h>\n#include <string.h>\n\n\n#include \"vex.h\"\n\nusing namespace vex;\n\n// Brain should be defined by default\nbrain Brain;\n\n\n// START IQ MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END IQ MACROS\n\n\n// Robot configuration code.\nmotor LeftMotor = motor(PORT1, false);\nmotor RightMotor = motor(PORT7, true);\ncontroller Controller = controller();\nmotor LeftRollers = motor(PORT6, false);\nmotor RightRollers = motor(PORT12, false);\nmotor BackLeftRollers = motor(PORT5, false);\nmotor BackRightRollers = motor(PORT11, false);\npneumatic LeftPneumatics = pneumatic(PORT4);\npneumatic RightPneumatics = pneumatic(PORT10);\ntouchled DEBUGLight = touchled(PORT8);\nbumper Bumper2 = bumper(PORT2);\ndistance Distance3_new = distance(PORT3);\n\n\n\n// Generated code.\n\n\n\n// define variable for remote controller enable/disable\nbool RemoteControlCodeEnabled = true;\n\n\n\n// Include the IQ Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nfloat myVariable, Deadband, MaxTorque, DrivingBlocked, DistanceControlMode;\n\n// \"when Controller ButtonRUp pressed\" hat block\nvoid onevent_ControllerButtonRUp_pressed_0() {\n  DEBUGLight.setColor(green);\n  DrivingBlocked = 1.0;\n  LeftMotor.setVelocity(100.0, percent);\n  RightMotor.setVelocity(100.0, percent);\n}\n\n// \"when Bumper2 pressed\" hat block\nvoid onevent_Bumper2_pressed_0() {\n  DEBUGLight.setColor(yellow);\n  DrivingBlocked = 0.0;\n  LeftRollers.setVelocity(100.0, percent);\n  RightRollers.setVelocity(-100.0, percent);\n  BackLeftRollers.setVelocity(100.0, percent);\n  BackRightRollers.setVelocity(-100.0, percent);\n  LeftPneumatics.extend(cylinder1);\n  wait(2.0, seconds);\n  LeftPneumatics.retract(cylinder1);\n}\n\n// \"when Controller ButtonFDown pressed\" hat block\nvoid onevent_ControllerButtonFDown_pressed_0() {\n  RightPneumatics.extend(cylinder1);\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  Brain.playSound(tada);\n  DistanceControlMode = 0.0;\n  Deadband = 5.0;\n  MaxTorque = 50.0;\n  RightMotor.setStopping(brake);\n  LeftMotor.setStopping(brake);\n  LeftMotor.setMaxTorque(MaxTorque, percent);\n  RightMotor.setMaxTorque(MaxTorque, percent);\n  LeftRollers.setMaxTorque(MaxTorque, percent);\n  RightRollers.setMaxTorque(MaxTorque, percent);\n  BackLeftRollers.setMaxTorque(MaxTorque, percent);\n  BackRightRollers.setMaxTorque(MaxTorque, percent);\n  LeftPneumatics.pumpOn();\n  LeftMotor.spin(forward);\n  RightMotor.spin(forward);\n  LeftRollers.spin(forward);\n  RightRollers.spin(forward);\n  BackLeftRollers.spin(forward);\n  BackRightRollers.spin(forward);\n  RightPneumatics.extend(cylinder2);\n  DEBUGLight.setColor(green);\n  DEBUGLight.setBrightness(100);\n  LeftRollers.setVelocity(0.0, percent);\n  RightRollers.setVelocity(0.0, percent);\n  BackLeftRollers.setVelocity(0.0, percent);\n  BackRightRollers.setVelocity(0.0, percent);\n  while (true) {\n    while ((DrivingBlocked == 0.0)) {\n      // Live driver control, responding to sticks\n      if (fabs(static_cast<float>(Controller.AxisA.position())) + fabs(static_cast<float>(Controller.AxisB.position() + fabs(static_cast<float>(Controller.AxisC.position())))) > Deadband) {\n        LeftMotor.setVelocity((Controller.AxisA.position() - Controller.AxisC.position()), percent);\n        RightMotor.setVelocity((Controller.AxisA.position() + Controller.AxisC.position()), percent);\n      }\n      else {\n        LeftMotor.setVelocity(0.0, percent);\n        RightMotor.setVelocity(0.0, percent);\n      }\n    wait(20, msec);\n    }\n  wait(20, msec);\n  }\n  return 0;\n}\n\n// \"when Controller ButtonFUp pressed\" hat block\nvoid onevent_ControllerButtonFUp_pressed_0() {\n  RightPneumatics.retract(cylinder1);\n}\n\n// \"when started\" hat block\nint whenStarted2() {\n  while (true) {\n    if (Distance3_new.objectDistance(inches) < 11.0 && DistanceControlMode == 1.0) {\n      DEBUGLight.setColor(blue);\n      DrivingBlocked = 1.0;\n      LeftMotor.stop();\n      RightMotor.stop();\n    }\n  wait(20, msec);\n  }\n  return 0;\n}\n\n// \"when Controller ButtonEUp pressed\" hat block\nvoid onevent_ControllerButtonEUp_pressed_0() {\n  LeftPneumatics.retract(cylinder1);\n}\n\n// \"when Controller ButtonRDown pressed\" hat block\nvoid onevent_ControllerButtonRDown_pressed_0() {\n  DistanceControlMode = 1.0;\n}\n\n// \"when Controller ButtonRDown released\" hat block\nvoid onevent_ControllerButtonRDown_released_0() {\n  DistanceControlMode = 0.0;\n  DEBUGLight.setColor(red);\n  LeftMotor.spin(forward);\n  RightMotor.spin(forward);\n  DrivingBlocked = 0.0;\n}\n\n// \"when Controller ButtonEUp pressed\" hat block\nvoid onevent_ControllerButtonEUp_pressed_1() {\n}\n\n// \"when Controller ButtonEDown pressed\" hat block\nvoid onevent_ControllerButtonEDown_pressed_0() {\n}\n\n// \"when Controller ButtonLDown pressed\" hat block\nvoid onevent_ControllerButtonLDown_pressed_0() {\n  LeftRollers.setVelocity(0.0, percent);\n  RightRollers.setVelocity(0.0, percent);\n  BackLeftRollers.setVelocity(0.0, percent);\n  BackRightRollers.setVelocity(0.0, percent);\n}\n\n// \"when Controller ButtonLUp pressed\" hat block\nvoid onevent_ControllerButtonLUp_pressed_0() {\n  LeftRollers.setVelocity(100.0, percent);\n  RightRollers.setVelocity(-100.0, percent);\n  BackLeftRollers.setVelocity(100.0, percent);\n  BackRightRollers.setVelocity(-100.0, percent);\n}\n\n\nint main() {\n  // register event handlers\n  Controller.ButtonRUp.pressed(onevent_ControllerButtonRUp_pressed_0);\n  Bumper2.pressed(onevent_Bumper2_pressed_0);\n  Controller.ButtonFDown.pressed(onevent_ControllerButtonFDown_pressed_0);\n  Controller.ButtonFUp.pressed(onevent_ControllerButtonFUp_pressed_0);\n  Controller.ButtonEUp.pressed(onevent_ControllerButtonEUp_pressed_0);\n  Controller.ButtonEUp.pressed(onevent_ControllerButtonEUp_pressed_1);\n  Controller.ButtonRDown.pressed(onevent_ControllerButtonRDown_pressed_0);\n  Controller.ButtonRDown.released(onevent_ControllerButtonRDown_released_0);\n  Controller.ButtonEDown.pressed(onevent_ControllerButtonEDown_pressed_0);\n  Controller.ButtonLDown.pressed(onevent_ControllerButtonLDown_pressed_0);\n  Controller.ButtonLUp.pressed(onevent_ControllerButtonLUp_pressed_0);\n\n  wait(15, msec);\n  vex::task ws1(whenStarted2);\n  whenStarted1();\n}","target":"Physical"}